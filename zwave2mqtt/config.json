{
  "name": "Z-Wave to MQTT",
  "version": "3.2.2.10",
  "slug": "zwave2mqtt",
  "description": "Fully configurable Z-Wave to MQTT gateway and control panel",
  "url": "https://github.com/hassio-addons/addon-zwave2mqtt",
  "ingress": true,
  "ingress_port": 8091,
  "panel_icon": "mdi:z-wave",
  "panel_title": "Z-Wave MQTT",
  "startup": "system",
  "init": false,
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "ports": {
    "8091/tcp": null
  },
  "ports_description": {
    "8091/tcp": "Not Required For Ingress"
  },
  "boot": "auto",
  "auth_api": true,
  "hassio_api": true,
  "hassio_role": "default",
  "auto_uart": true,
  "map": ["config:rw", "share:rw", "ssl:rw"],
  "options": {
    "ssl": true,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem"
  },
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)?",
    "ssl": "bool",
    "certfile": "str",
    "keyfile": "str",
    "leave_front_door_open": "bool?"
  }
}
